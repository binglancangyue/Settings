    <string name="security_settings_faceid_enroll_onboard_title">人臉設置</string>
    <string name="security_settings_faceid_enroll_onboard_message">"要使用您的人臉解鎖屏幕或確認購買交易，您需要："</string>
    <string name="security_settings_faceid_enroll_onboard_message_1" >"設置備用屏幕鎖定方式"</string>
