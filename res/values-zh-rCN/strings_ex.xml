    <string name="security_settings_faceid_enroll_onboard_title">人脸设置</string>
    <string name="security_settings_faceid_enroll_onboard_message">"要使用您的人脸解锁屏幕或确认购买交易，您需要："</string>
    <string name="security_settings_faceid_enroll_onboard_message_1" >"设置备用屏幕锁定方式"</string>
